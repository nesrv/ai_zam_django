# Пример файла .env для AI-ZAM-DJANGO
# Скопируйте этот файл в .env и заполните своими значениями

# Токен Telegram бота (обязательно)
TELEGRAM_TOKEN=5555555555:AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA

# Интервал опроса Telegram API в секундах
TELEGRAM_POLL_INTERVAL=20

# Ключ API DeepSeek (опционально)
DEEPSEEK_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Настройки Django
DEBUG=False
SECRET_KEY=your-secret-key-here
ALLOWED_HOSTS=ai-zam.ru,www.ai-zam.ru